package com.searun.orderoperation.entity;

import java.io.Serializable;

/**
 * 附近车源信息
 * 
 * @author zhazhaobao
 * 
 */
public class AroundCarInfo implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 3680713247193174213L;

	private String url;

	/**
	 * 头像icon
	 */
	private String imageIcon;

	/**
	 * 描述文字
	 */
	private String description;

	/**
	 * 经度
	 */
	private double latitude;

	/**
	 * 维度
	 */
	private double longitude;

	/**
	 * 期望流向
	 */
	private String target;

	/**
	 * 用户名
	 */
	private String usrName;

	/**
	 * 手机号
	 */
	private String phoneNo;

	/**
	 * 车牌
	 */
	private String carID;

	/**
	 * 车型
	 */
	private String carStyle;

	/**
	 * 车长
	 */
	private String carLength;

	/**
	 * 载重
	 */
	private String carWeigth;

	/**
	 * 距离
	 */
	private String distance;

	/**
	 * 时间
	 */
	private String time;

	/**
	 * 星级
	 */
	private int star;

	private boolean isAuthentication = false;

	public String getTarget() {
		return target;
	}

	public void setTarget(String target) {
		this.target = target;
	}

	public String getUsrName() {
		return usrName;
	}

	public void setUsrName(String usrName) {
		this.usrName = usrName;
	}

	public String getPhoneNo() {
		return phoneNo;
	}

	public void setPhoneNo(String phoneNo) {
		this.phoneNo = phoneNo;
	}

	public String getCarID() {
		return carID;
	}

	public void setCarID(String carID) {
		this.carID = carID;
	}

	public String getCarStyle() {
		return carStyle;
	}

	public void setCarStyle(String carStyle) {
		this.carStyle = carStyle;
	}

	public String getCarLength() {
		return carLength;
	}

	public void setCarLength(String carLength) {
		this.carLength = carLength;
	}

	public String getCarWeigth() {
		return carWeigth;
	}

	public void setCarWeigth(String carWeigth) {
		this.carWeigth = carWeigth;
	}

	public String getDistance() {
		return distance;
	}

	public void setDistance(String distance) {
		this.distance = distance;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getImageIcon() {
		return imageIcon;
	}

	public void setImageIcon(String imageIcon) {
		this.imageIcon = imageIcon;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public double getLatitude() {
		return latitude;
	}

	public void setLatitude(double latitude) {
		this.latitude = latitude;
	}

	public double getLongitude() {
		return longitude;
	}

	public void setLongitude(double longitude) {
		this.longitude = longitude;
	}

	public int getStar() {
		return star;
	}

	public void setStar(int star) {
		this.star = star;
	}

	public boolean isAuthentication() {
		return isAuthentication;
	}

	public void setAuthentication(boolean isAuthentication) {
		this.isAuthentication = isAuthentication;
	}

}
